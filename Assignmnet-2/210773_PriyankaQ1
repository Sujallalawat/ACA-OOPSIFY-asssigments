#include <iostream>
using namespace std;

class Animal
{
public:
    void walk()
    {
        cout << "I am walking" << endl;
    }
};

class Bird : public Animal
{
public:
    void fly()
    {
        cout << "I am flying" << endl;
    }
};

int main()
{
    Bird bird;
    bird.walk();
    bird.fly();

    return 0;
}
